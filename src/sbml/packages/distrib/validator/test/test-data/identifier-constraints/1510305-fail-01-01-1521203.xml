<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" xmlns:distrib="http://www.sbml.org/sbml/level3/version2/distrib/version1" level="3" version="2" distrib:required="true">
  <model>
    <listOfFunctionDefinitions>
      <functionDefinition id="distribution">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> x </ci>
            </bvar>
            <notanumber/>
          </lambda>
        </math>
        <distrib:drawFromDistribution>
          <distrib:normalDistribution name="normal">
            <distrib:truncationLowerBound distrib:value="2.3" distrib:inclusive="true"/>
            <distrib:truncationUpperBound distrib:value="7.3" distrib:inclusive="true"/>
            <distrib:mean distrib:id="ss" distrib:var="mean"/>
            <distrib:stddev distrib:var="stddev"/>
          </distrib:normalDistribution>
          <distrib:listOfDistribInputs>
            <distrib:distribInput id="mean" distrib:index="0"/>
          </distrib:listOfDistribInputs>
        </distrib:drawFromDistribution>
      </functionDefinition>
    </listOfFunctionDefinitions>
  </model>
</sbml>
